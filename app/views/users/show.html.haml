= render 'shared/error_messages'
- case (@user.utype)
- when "student"
	= render 'student'
	%p student
- when "instructor"
	= render 'instructor'
	%p instructor
- when "sponsor"
	= render 'sponsor'
- when "admin"
	= render 'admin'
	%p admin
- else
	%h1 User type has not been set, please contact an admin to get this fixed.
