= form_for @instructor, url: { action: @url } do |f|
  = render 'shared/error_messages', object: @instructor
  %span.required *
  = f.label :email, "Email"
  = f.text_field :email
  %span.required *
  = f.label :first_name, "First Name"
  = f.text_field :first_name
  %span.required *
  = f.label :last_name, "Last Name"
  = f.text_field :last_name
  .bp1-wdn-col-one-third
    %span.required *
    = f.label "Class"
    %ol
      %li
        = f.radio_button :classname, "CSCE 486/488"
        = f.label :classname_csce_486488, "CSCE 486/488"
      %li
        = f.radio_button :classname, "CSCE 487/489"
        = f.label :classname_csce_487489, "CSCE 487/489"
  .bp1-wdn-col-two-thirds
    %span.required *
    = f.label "Semester"
    %ol
      %li
        = f.radio_button :semester, "Fall"
        = f.label :semester_fall, "Fall"
      %li
        = f.radio_button :semester, "Spring"
        = f.label :semester_spring, "Spring"
  %ol
    %li
    %span.required *
    denotes required field
  = f.submit "Submit"