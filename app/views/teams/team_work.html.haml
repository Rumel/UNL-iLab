%h1 Team Work
-#= render 'shared/error_messages'

.zenbox
  =form_tag(team_track_teams_path, remote: true, id: "team_work") do
    = hidden_field_tag :authenticity_token, form_authenticity_token
    .inline{style: "width: 35%"}
      =select_tag :id, options_for_select(@teams.map{|team| [team.name, team.id]}), :class => "no-margin"
    .inline
    =submit_tag "Track"
.wdn-main#maincontent
